<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.pet.sns.model.dto.Member"   		alias="member"/>
		<typeAlias type="com.pet.sns.model.dto.Post"   		alias="post"/>
		<typeAlias type="com.pet.sns.model.dto.Comment"   	alias="comment"/>
		<typeAlias type="com.pet.sns.model.dto.Pet"   		alias="pet"/>
