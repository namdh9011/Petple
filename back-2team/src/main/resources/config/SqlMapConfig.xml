		<typeAlias type="com.pet.sns.model.dto.Likepost"   		alias="likepost"/>
	</typeAliases>
	<mappers>
	 <mapper resource="config/MemberMapper.xml" />
	 <mapper resource="config/CommentQuery.xml" />
	 <mapper resource="config/PostQuery.xml" />
